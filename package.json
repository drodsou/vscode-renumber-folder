{
	"name": "renumber-folder",
	"displayName": "Renumber folder",
	"description": "Renumber folder files adding 00# prefix.",
	"version": "1.0.0",
	"publisher": "drodsou",
	"icon": "icon.png",
	"repository": {
		"type" : "git",
		"url": "https://github.com/drodsou/vscode-renumber-folder"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:renumber-folder.renumber-folder"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "renumber-folder.renumber-folder",
				"title": "Renumber Folder"
			}
		],
		"keybindings": [
			{
				"command": "renumber-folder.renumber-folder",
				"key": "ctrl+shift+3",
				"mac": "shift+cmd+3"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
